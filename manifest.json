{
    "name": "bear",
    "version": "1.0",
    
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "16": "16.png"
    },
    "browser_action": {
      "default_popup":"popup.html",
      "default_title": "Bear"
    },
    "background": {
      "scripts": ["background.js"]
    },
    "permissions": ["tabs","downloads","contextMenus"],
    "manifest_version": 2
  }